    <ImageView
        android:id="@+id/iv_viewport"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_gravity="center"
        android:src="@drawable/cad_op_zuoshitu"
        android:visibility="gone" />
</FrameLayout>

