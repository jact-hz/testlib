    android:layout_height="47dp">

    <TextView
        android:id="@+id/tv_cad_range"
        style="@style/cad_bottom_op_menu_style"
        android:layout_gravity="center"
        android:drawableTop="@drawable/cad_op_selor_space"
        android:text=""
